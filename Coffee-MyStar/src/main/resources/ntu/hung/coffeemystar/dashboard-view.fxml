<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="ntu.hung.coffeemystar.Dashboard_Controller"
            prefHeight="720" prefWidth="980">
    <PieChart fx:id="salesChart" layoutX="50.0" layoutY="50.0" prefWidth="400" prefHeight="400"/>

    <Label fx:id="totalSalesLabel" layoutX="500.0" layoutY="100.0" style="-fx-font-size: 16; -fx-font-weight: bold;"/>

    <Label fx:id="lowStockLabel" layoutX="500.0" layoutY="150.0" style="-fx-font-size: 14; -fx-font-weight: normal;"/>
</AnchorPane>
